alias ls='ls --color=tty'
alias en='emacs -nw'
alias grep='grep --binary-files=without-match --directories=skip'
alias gos='go-search'

alias gc='git commit'
alias gd='git diff'
alias gst='git status'
alias gca='git commit -a'
alias gcam='git commit -am'

alias gcls='gcloud compute instances list'

function gcssh {
